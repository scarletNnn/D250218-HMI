<Page
    x:Class="D250218.Views.Input1View"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:customcontrols="clr-namespace:D250218.CustomControls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:D250218.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    xmlns:vm="clr-namespace:D250218.ViewModels"
    Title="Input1View"
    Width="1700"
    Height="1000"
    d:DataContext="{d:DesignInstance Type=vm:Input1ViewModel}"
    Background="AntiqueWhite"
    mc:Ignorable="d">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="2" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="2" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="2" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <StackPanel>
            <customcontrols:InputOutputBox
                Margin="0,10,0,0"
                ControlName="机器人放料位治具有无检测123"
                Data="{Binding ModbusService.DataIN[0]}"
                SerialNumber="IN_000" />
            <customcontrols:InputOutputBox
                Margin="0,10,0,0"
                ControlName="机器人上料位治具有无检测123"
                Data="{Binding ModbusService.DataIN[1]}"
                SerialNumber="IN_001" />
            <customcontrols:InputOutputBox
                Margin="0,10,0,0"
                ControlName="机器人放料位治具有无检测123"
                Data="{Binding ModbusService.DataIN[2]}"
                SerialNumber="IN_002" />
            <customcontrols:InputOutputBox
                Margin="0,10,0,0"
                ControlName="机器人上料位治具有无检测123"
                Data="{Binding ModbusService.DataIN[3]}"
                SerialNumber="IN_003" />
            <customcontrols:InputOutputBox
                Margin="0,10,0,0"
                ControlName="机器人放料位治具有无检测123"
                Data="{Binding ModbusService.DataIN[4]}"
                SerialNumber="IN_004" />
            <customcontrols:InputOutputBox
                Margin="0,10,0,0"
                ControlName="机器人上料位治具有无检测123"
                Data="{Binding ModbusService.DataIN[5]}"
                SerialNumber="IN_005" />
        </StackPanel>
        <Separator
            Grid.Column="1"
            BorderBrush="LightCoral"
            BorderThickness="1">
            <Separator.LayoutTransform>
                <RotateTransform Angle="90" />
            </Separator.LayoutTransform>
        </Separator>
        <StackPanel Grid.Column="2">
            <customcontrols:InputOutputBox
                Margin="0,10,0,0"
                Command="{Binding FooCommand}"
                CommandParameter="1"
                ControlName="机器人放料位治具有无检测123"
                Data="{Binding Data}"
                SerialNumber="IN_001" />
            <customcontrols:InputOutputBox
                Margin="0,10,0,0"
                Command="{Binding Foo2Command}"
                CommandParameter="1"
                ControlName="机器人上料位治具有无检测123"
                Data="{Binding Data2}"
                SerialNumber="IN_002" />
        </StackPanel>
        <Separator
            Grid.Column="3"
            BorderBrush="LightCoral"
            BorderThickness="1">
            <Separator.LayoutTransform>
                <RotateTransform Angle="90" />
            </Separator.LayoutTransform>
        </Separator>
        <StackPanel Grid.Column="4">
            <customcontrols:InputOutputBox
                Margin="0,10,0,0"
                Command="{Binding FooCommand}"
                CommandParameter="1"
                ControlName="机器人放料位治具有无检测123"
                Data="{Binding Data}"
                SerialNumber="IN_001" />
            <customcontrols:InputOutputBox
                Margin="0,10,0,0"
                Command="{Binding Foo2Command}"
                CommandParameter="1"
                ControlName="机器人上料位治具有无检测123"
                Data="{Binding Data2}"
                SerialNumber="IN_002" />
        </StackPanel>
        <Separator
            Grid.Column="5"
            BorderBrush="LightCoral"
            BorderThickness="1">
            <Separator.LayoutTransform>
                <RotateTransform Angle="90" />
            </Separator.LayoutTransform>
        </Separator>
        <StackPanel Grid.Column="6">
            <customcontrols:InputOutputBox
                Margin="0,10,0,0"
                Command="{Binding FooCommand}"
                CommandParameter="1"
                ControlName="机器人放料位治具有无检测123"
                Data="{Binding Data}"
                SerialNumber="IN_001" />
            <customcontrols:InputOutputBox
                Margin="0,10,0,0"
                Command="{Binding Foo2Command}"
                CommandParameter="1"
                ControlName="机器人上料位治具有无检测123"
                Data="{Binding Data2}"
                SerialNumber="IN_002" />
        </StackPanel>
    </Grid>
</Page>
